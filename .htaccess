ErrorDocument 403 /blogAlaska/view/error.php
ErrorDocument 404 /blogAlaska/view/error.php
ErrorDocument 405 /blogAlaska/view/error.php
ErrorDocument 408 /blogAlaska/view/error.php
ErrorDocument 500 /blogAlaska/view/error.php
ErrorDocument 502 /blogAlaska/view/error.php
ErrorDocument 504 /blogAlaska/view/error.php

RewriteEngine On
RewriteRule ^([^_]*)_([^_]*)\.html$ /blogAlaska/?p=$1&a=$2 [L]
RewriteRule ^([^_]*)_([^_]*)_([^_]*)\.html$ /blogAlaska/?p=$1&a=$2&id=$3 [L]
RewriteRule ^([^_]*)_([^_]*)_([^_]*)_([^_]*)_([^_]*)\.html$ /blogAlaska/?p=$1&a=$2&postid=$3&commentId=$4&vote=$5 [L]
